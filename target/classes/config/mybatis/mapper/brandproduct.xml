<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="brandproduct">

	
	<select id="selectList" resultType="brandproduct">
		select * from brand_product
	</select>
	
	<select id="selectOne" resultType="brandproduct" parameterType="int">
		select * from brand_product where idx=#{idx}
	</select>
	
	<insert id="insert" parameterType="brandproduct">
		insert into brand_product values(brand_product_seq.nextval,#{brand_idx},#{bname},#{introduce},#{othershop})
	</insert>
	
	<update id="update" parameterType="brandproduct">
		update brand_product set brand_idx=#{brand_idx},name=#{name},introduce=#{introduce},othershop=#{othershop} where idx=#{idx}
	</update>
	
	<delete id="delete" parameterType="int">
		delete brand_product where idx=#{idx}
	</delete>

</mapper>













